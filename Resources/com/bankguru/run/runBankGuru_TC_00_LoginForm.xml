    <classes>
      		<class name="com.bankguru.account.RegisterLogin_Global"/>
           <class name="com.bankguru.login.Login_01_ValidateLoginForm">
           
            </class>
    </classes>
  </test> 
  <test name="Run on INTERNET EXPLOER">
  <parameter name="browser" value="ie"/>
    <classes>
      		<class name="com.bankguru.account.RegisterLogin_Global"/>
           <class name="com.bankguru.login.Login_01_ValidateLoginForm">
           
            </class>
    </classes>
  </test> 
  <test name="Run on CHROME HEADLESS">
  <parameter name="browser" value="chromeheadless"/>
    <classes>
      		<class name="com.bankguru.account.RegisterLogin_Global"/>
           <class name="com.bankguru.login.Login_01_ValidateLoginForm">
           
            </class>
    </classes>
  </test> 
  
</suite> 
